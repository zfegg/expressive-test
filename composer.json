{
    "name": "zfegg/expressive-test",
    "description": "Zend Expressive abstract test case for PHPUnit",
    "require": {
        "phpunit/phpunit": "^6.0 | ^7.0 | ^8.0",
        "zendframework/zend-expressive": "^3.0",
        "zendframework/zend-diactoros": "^1.7 | ^2.0",
        "zendframework/zend-servicemanager": "^3.3",
        "dflydev/fig-cookies": "^2.0"
    },
    "license": "MIT",
    "keywords": [
        "zend",
        "expressive",
        "test",
        "phpunit"
    ],
    "authors": [
        {
            "name": "Moln",
            "email": "moln.xie@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zfegg\\ExpressiveTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZfeggTest\\ExpressiveTest\\": "test/"
        }
    },
    "require-dev": {
        "ext-json": "*",
        "zendframework/zend-coding-standard": "^1.0",
        "zendframework/zend-config-aggregator": "^1.1",
        "zendframework/zend-expressive-fastroute": "^3.0"
    }
}
